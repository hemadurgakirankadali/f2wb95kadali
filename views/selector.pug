extends layout
block content

 h1= title

  - let earbuds_img_names = ["applebuds.jpg","boatearbuds.jpg","oneplus.jpg","onepluswhite.jpg","realme.jpg"]
  - for(let i=0;i<3;i++){
      - let img = Math.floor(Math.random()*5);
      - let item = earbuds_img_names[img];
      img(src=`/images/${item}` alt=`${item}` style='height:300px; width:300px') 
  - }
 