extends layout
block content
  h1= title
  p Kiran Kadali Lab10 #{title}

  button(type="button", class="btn btn-dark",onClick="doBump()") Press Me
  <br>
  div(id="bump") Bump the count
  script.
    var count=0;
    var bump=1;
    function doBump() {
      count=count+bump;
      bump=bump+1;
      document.getElementById("bump").innerHTML=`Count ${count} with bump ${bump}.`;
    }


